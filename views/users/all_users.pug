extends ../templates/layout
block content
  .content.mt-3
    .animated.fadeIn
      .row
        .col-md-12
          .card
            .card-header
              strong.card-title All Users
            .card-body
              table#bootstrap-data-table.table.table-striped.table-bordered
                thead
                  tr
                    th ID
                    th Nom
                    th Email
                    th Type
                    th Option
                tbody
                  each key in jsonData
                    tr
                      td !{key.id}
                      td !{key.name}
                      td !{key.email}
                      td !{key.type}
                      td 
                        a.btn-outline-success(href='/edit')
                          i.fa.fa-edit
